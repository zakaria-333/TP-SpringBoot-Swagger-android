<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/etudiant"
        android:layout_width="232dp"
        android:layout_height="56dp"
        android:layout_marginBottom="72dp"
        android:text="Gestion des etudiants"
        app:layout_constraintBottom_toTopOf="@+id/filiere"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/filiere"
        android:layout_width="228dp"
        android:layout_height="52dp"
        android:layout_marginBottom="100dp"
        android:text="Gestion des filieres"
        app:layout_constraintBottom_toTopOf="@+id/lef"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/lef"
        android:layout_width="221dp"
        android:layout_height="61dp"
        android:layout_marginBottom="256dp"
        android:text="Liste d'etudiants par filiere"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>